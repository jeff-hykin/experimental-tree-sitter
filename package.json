{
	"name": "experimental-tree-sitter",
	"displayName": "Experimental Tree Sitter",
	"description": "Syntax highlighting based on Tree-sitter",
	"homepage": "https://github.com/jeff-hykin/tree-sitter",
	"version": "0.1.1",
	"publisher": "jeff-hykin",
	"license": "MIT",
	"icon": "images/syntax.png",
	"repository": {
		"url": "https://github.com/jeff-hykin/tree-sitter"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"debug": "echo debugging"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Other"
	],
	"keywords": [
		"Syntax",
		"Highlighter",
		"Highlight",
		"Highlighting",
		"Coloring",
		"Tree-Sitter",
		"Parser",
		"C",
		"C++",
		"CPP"
	],
	"engines": {
		"vscode": "^1.15.0"
	},
	"activationEvents": [
		"onLanguage:c",
		"onLanguage:cpp"
	],
	"main": "./extension.js",
	"dependencies": {
		"cson": "^5.1.0",
		"js-yaml": "^3.13.1",
		"jsonc-parser": "^2.1.0",
		"tree-sitter-c": "^0.13.13",
		"tree-sitter-cpp": "^0.13.15",
		"tree-sitter-css": "^0.13.7",
		"tree-sitter-go": "^0.13.3",
		"tree-sitter-html": "^0.13.5",
		"tree-sitter-javascript": "^0.13.10",
		"tree-sitter-python": "^0.14.0",
		"tree-sitter-ruby": "^0.13.14",
		"tree-sitter-typescript": "^0.13.6",
		"web-tree-sitter": "^0.15.2"
	},
	"devDependencies": {
		"@types/node": "^12.0.2",
		"tslint": "^5.16.0",
		"typescript": "^3.4.5",
		"vsce": "^1.61.0",
		"vscode": "^1.1.34"
	},
	"contributes": {
		"colors": [
			{
				"id": "syntax.type",
				"description": "Color for types",
				"defaults": {
					"dark": "#26A69A",
					"light": "#26A69A",
					"highContrast": "#26A69A"
				}
			},
			{
				"id": "syntax.namespace",
				"description": "Color for namespaces",
				"defaults": {
					"dark": "#00897B",
					"light": "#00897B",
					"highContrast": "#00897B"
				}
			},
			{
				"id": "syntax.function",
				"description": "Color for functions and methods",
				"defaults": {
					"dark": "#00BCD4",
					"light": "#00BCD4",
					"highContrast": "#00BCD4"
				}
			},
			{
				"id": "syntax.variable",
				"description": "Color for variables and fields",
				"defaults": {
					"dark": "#42A5F5",
					"light": "#42A5F5",
					"highContrast": "#42A5F5"
				}
			},
			{
				"id": "syntax.string",
				"description": "Color for string literals",
				"defaults": {
					"dark": "#90A4AE",
					"light": "#90A4AE",
					"highContrast": "#90A4AE"
				}
			},
			{
				"id": "syntax.number",
				"description": "Color for number literals",
				"defaults": {
					"dark": "#90A4AE",
					"light": "#90A4AE",
					"highContrast": "#90A4AE"
				}
			},
			{
				"id": "syntax.punctuation",
				"description": "Color for punctuation symbols",
				"defaults": {
					"dark": "#A1887F",
					"light": "#A1887F",
					"highContrast": "#A1887F"
				}
			},
			{
				"id": "syntax.comment",
				"description": "Color for comments",
				"defaults": {
					"dark": "#546E7A",
					"light": "#546E7A",
					"highContrast": "#546E7A"
				}
			},
			{
				"id": "syntax.keyword_constant",
				"description": "Color for language constants (true, nullptr)",
				"defaults": {
					"dark": "#A89F9B",
					"light": "#A89F9B",
					"highContrast": "#A89F9B"
				}
			},
			{
				"id": "syntax.keyword_directive",
				"description": "Color for directive keywords (#include, import)",
				"defaults": {
					"dark": "#7E57C2",
					"light": "#7E57C2",
					"highContrast": "#7E57C2"
				}
			},
			{
				"id": "syntax.keyword_control",
				"description": "Color for control flow keywords (if, continue)",
				"defaults": {
					"dark": "#7986CB",
					"light": "#7986CB",
					"highContrast": "#7986CB"
				}
			},
			{
				"id": "syntax.keyword_operator",
				"description": "Color for operators (+=, ->)",
				"defaults": {
					"dark": "#9575CD",
					"light": "#9575CD",
					"highContrast": "#9575CD"
				}
			},
			{
				"id": "syntax.storage_modifier",
				"description": "Color for storage modifiers (const, public)",
				"defaults": {
					"dark": "#00897B",
					"light": "#00897B",
					"highContrast": "#00897B"
				}
			}
		]
	},
	"enableProposedApi": true
}
